{
  "scripts": {
    "start": "yarn dev",
    "dev": "node server.js",
    "test": "jest --coverage",
    "test:watch": "yarn test -- --watch",
    "lint": "standard --parser babel-eslint --verbose | snazzy",
    "lint:watch": "gulp",
    "lint:fix": "standard --fix",
    "build": "cross-env NODE_ENV=production BABEL_ENV=production webpack --progress --config webpack/prod.config -p",
    "build:analyzer": "cross-env ANALYZER=true yarn build",
    "storybook": "start-storybook -p 6006 -c storybook",
    "storybook:build": "cross-env NODE_ENV=production BABEL_ENV=production build-storybook -c storybook",
    "update-packages": "node scripts/update.js"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/components/**/*.js",
      "src/utils/**/*.js",
      "!src/components/index.js",
      "!src/**/*.story.js",
      "!src/index.js",
      "!config/*.js"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "^.+\\.(styl|svg|png)$": "<rootDir>/config/css-stub.js"
    },
    "setupFiles": [
      "raf/polyfill",
      "<rootDir>/config/setup-tests.js"
    ]
  },
  "standard": {
    "globals": [
      "it",
      "expect",
      "jest"
    ]
  },
  "devDependencies": {
    "@storybook/react": "^3.2.12",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.1.2",
    "clean-webpack-plugin": "^0.1.17",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.2",
    "enzyme-to-json": "^3.1.4",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.5",
    "gulp": "^3.9.1",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^21.2.1",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-test-renderer": "^16.0.0",
    "snazzy": "^7.0.0",
    "standard": "^10.0.3",
    "standard-loader": "^6.0.1",
    "style-loader": "^0.19.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "url-loader": "^0.6.2",
    "webpack": "^3.7.1",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dashboard": "^1.0.0-5",
    "webpack-dev-server": "^2.9.1"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "babel-runtime": "^6.26.0",
    "moment": "^2.19.1",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "strclass": "^2.0.9"
  }
}
